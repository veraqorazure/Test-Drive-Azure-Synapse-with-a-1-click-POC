{
	"name": "Dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Test",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "Test",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "lookup1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          CountryId as integer,",
				"          Name as string,",
				"          NameShort as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table',",
				"     staged: true) ~> source1",
				"source(output(",
				"          CountryId as integer,",
				"          Countrykey as integer,",
				"          ValidFrom as timestamp,",
				"          ValidTo as timestamp,",
				"          IsActive as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table',",
				"     staged: true) ~> source2",
				"source1 derive(ActiveStatus = 1) ~> derivedColumn1",
				"derivedColumn1, source2 lookup(source1@CountryId == Countrykey,",
				"     multiple: true,",
				"     broadcast: 'auto')~> lookup1"
			]
		}
	}
}